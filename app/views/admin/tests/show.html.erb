<h1>
  Test:
  <%= @test.title %>
</h1>

<h3>
  Level:
  <%= @test.level %>
</h3>

<h3>
  Category:
  <%= @test.category.title %>
</h3>

<table>
  <tr>
    <th>Body</th>
    <th>Test</th>
    <th colspan="3">Actions</th>
  </tr>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
  <%= render @questions, test: @test %>
</table>
<%= 'NO QUESTIONS' if @test.questions.blank? %><br>
<%= link_to t('actions.back'), admin_tests_path %>